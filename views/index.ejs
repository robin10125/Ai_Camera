<!DOCTYPE html>
<html>
  <head>
    <title>Ai-Camera</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <!--Javascript socket io--> 
    <script src="/socket.io/socket.io.js"></script>
    <!-- Import TensorFlow.js library -->
    <script src="https://cdn.jsdelivr.net/npm/@tensorflow/tfjs@2.0.0/dist/tf.min.js"></script>
    <script type="text/javascript" charset="utf-8">
      var socket = io();
      socket.on('connect', function() {
        socket.emit('my event', {data: 'I\'m connected!'});
      });
    </script>

    <script src="/javascripts/app.js" defer></script>
    
    <div id = 'header'><h1>Ai Camera</h1> </div>

    <div id = 'outer-container' class= 'container'>
      
      <div id = 'console-container' class = 'card'>
        <select id = 'models'>
          <option value = 'impressionism'>Impressionism</option>
          <option value = 'impressionism2'>Impressionism2</option>
        </select>
        <input id = 'input' type = 'file'>
        <button id = 'submit'class="btn btn-primary" >Submit</button>
      </div>
    
      <div id = 'image-container' >
        <img id = 'thumbnail' src = '#'>
      </div>

      <div id = 'canvas-container'>
        <canvas id = 'prediction'></canvas>
      </div>

    </div>
   
    <!--import bootstrap-->
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>

